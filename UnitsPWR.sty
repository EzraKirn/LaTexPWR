\NeedsTeXFormat{LaTeX2e}
\ProvidesPackage{UnitsPWR}[Custom units symbols for PWR reports]
\RequirePackage{ xparse }

\NewDocumentCommand{\mmTOm}{m}{
    \ensuremath{\frac{#1mm}{1000}}
}

\NewDocumentCommand{\gTOkg}{m}{
    \ensuremath{\frac{#1g}{1000}}
}

\NewDocumentCommand{\AREA}{O{}}{
    \ensuremath{ { #1m }^2 }
}

\NewDocumentCommand{\VOLUME}{O{}}{
    \ensuremath{ { #1m }^3 }
}

\NewDocumentCommand{\VELOCITY}{O{} O{s}}{
    \ensuremath{\frac{#1m}{#2}}
}

\newcommand{\ACCELERATION}{
    \ensuremath{\frac{m}{s^2}}
}

\NewDocumentCommand{\FLOWRATEvol}{O{} O{s}}{
    \ensuremath{\frac{{#1m}^3}{#2}}
}

\NewDocumentCommand{\FLOWRATEmass}{O{k} O{s}}{
    \ensuremath{\frac{#1g^3}{#2}}
}

\newcommand{\CONVECTIONa}{
    \ensuremath{\frac{W}{m^2 \cdot K}}
}

\newcommand{\COMDUCTIONl}{
    \ensuremath{\frac{W}{m \cdot K}}
}

\newcommand{\TMPERATURE}{
    \ensuremath{^\circ C}
}

\newcommand{\VISCOSITYkinematic}{
    \ensuremath{\frac{m^2}{s}}
}

\newcommand{\VISCOSITYdynamic}{
    \ensuremath{Pa \cdot s}
}

\newcommand{\DENSITY}{
    \ensuremath{\frac{kg}{m^3}}
}

\newcommand{\SPECIFICHEAT}{
    \ensuremath{\frac{J}{kg \cdot K}}
}

\NewDocumentCommand{\ENTHALPY}{O{}}{
    \ensuremath{\frac{#1J}{kg}}
}

\NewDocumentCommand{\ENTHROPY}{O{}}{
    \ensuremath{\frac{#1J}{kg \cdot K}}
}




